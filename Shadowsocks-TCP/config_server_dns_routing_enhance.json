{
    "log": {
        "access": "",
        "error": "",
        "loglevel": "warning",
        "dnsLog": false
    },
    "dns": {
        "servers": [
            "quic+local://dns.adguard.com"
        ],
        "queryStrategy": "UseIPv4"
    },
    "routing": {
        "domainStrategy": "IPIfNonMatch",
        "rules": [
            {
                "type": "field",
                "domain": [
                    "geosite:category-ads-all",
                    "geosite:cn",
                    "geosite:category-games@cn"
                ],
                "outboundTag": "block"
            },
            {
                "type": "field",
                "ip": [
                    "geoip:cn",
                    "geoip:private"
                ],
                "outboundTag": "block"
            }
        ]
    },
    "inbounds": [
        {
            "listen": "0.0.0.0",
            "port": 50001,
            "protocol": "shadowsocks",
            "settings": {
                    "method": "AES-256-GCM",
                    "password": "8tetnZHnBJcrnbjIpTi9fQ==",
                    "network": "tcp",
                    "ivCheck": true
            },
            "streamSettings": {
                "network": "tcp",
                "security": "none"
            }
        }
    ],
    "outbounds": [
        {
            "protocol": "freedom",
            "settings": {
                "domainStrategy": "UseIPv4"
            },
            "tag": "direct"
        },
        {
            "protocol": "blackhole",
            "settings": {
                "response": {
                    "type": "http"
                }
            },
            "tag": "block"
        }
    ]
}
